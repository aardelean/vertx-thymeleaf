{
  "name": "react-vertx",
  "description": "React.js Google Maps integration component",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run lint && npm run clean",
    "build": "babel src --out-dir lib",
    "lint": "eslint src",
    "pretest:cov": "npm run lint",
    "pretest": "npm run lint",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover --report lcov _mocha -- $npm_package_config_mocha",
    "test:watch": "npm test -- --watch",
    "test": "mocha $npm_package_config_mocha"
  },
  "config": {
    "mocha": "--compilers js:babel/register ./src/**/__tests__/*.spec.js"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "codeclimate-test-reporter": "^0.1.1",
    "del": "^2.0.2",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "expect": "^1.12.1",
    "google-map-react": "^0.7.5",
    "gulp": "^3.9.0",
    "gulp-load-plugins": "^1.0.0",
    "gulp-sync": "^0.1.4",
    "gulp-util": "^3.0.6",
    "harmonize": "^1.4.4",
    "isparta": "^3.1.0",
    "istanbul": "^0.3.22",
    "mocha": "^2.3.3",
    "react": "^0.14.0",
    "jquery"  : "2.1.1",
    "react-dom": "^0.14.0",
    "rimraf": "^2.4.3",
    "tomchentw-npm-dev": "^3.1.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "can-use-dom": "^0.1.0",
    "google-maps-infobox": "^1.1.13",
    "invariant": "^2.1.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "peerDependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
